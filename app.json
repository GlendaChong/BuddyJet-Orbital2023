{
  "expo": {
    "scheme": "buddyjet",
    "owner": "buddyjet",
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "The app accesses your photos to let you attach them with your todos."
        }
      ]
    ],
    "name": "orbital-app", 
    "slug": "orbital-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.orbitalapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.orbitalapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "99a074de-8ec0-486e-b67e-bb306d7ec8d9"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }, 
    "updates": {
      "url": "https://u.expo.dev/99a074de-8ec0-486e-b67e-bb306d7ec8d9"
    }
  }
}
